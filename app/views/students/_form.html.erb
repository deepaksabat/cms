<%= simple_form_for(@student), html: { multipart: true } do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :address %>
    <%= f.input :rollno %>
    <%= f.input :dob %>
    <%= f.input :phno %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
